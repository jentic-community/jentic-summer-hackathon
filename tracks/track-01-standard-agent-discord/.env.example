# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DEV_GUILD_ID=your_discord_server_id

# Jentic Agent API Key (from app.jentic.com -> Agents)
JENTIC_AGENT_API_KEY=your_jentic_agent_api_key_here

# LLM Provider (choose ONE)
OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here

# Default LLM Model
LLM_MODEL=gpt-4

# Optional: Bot Configuration
BOT_PREFIX=!
BOT_DESCRIPTION=AI Agent Discord Bot powered by Standard Agent and Jentic